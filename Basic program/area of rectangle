//Area of rectangle program
import java.util.*;
class Main 
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the width");
        int w=sc.nextInt();
        System.out.println("enter the height");
        int h=sc.nextInt();
        int area=h*w;
        System.out.println("area is:"+area);
    }
}
