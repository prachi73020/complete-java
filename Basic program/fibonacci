import java.util.*;
class Main 
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int sum=0;
        int a=sc.nextInt();
        int b=sc.nextInt();
        int n=sc.nextInt();
        for(int i=0;i<n;i++)
        {
            sum=a+b;
            System.out.print(sum+" ");
            a=b;
            b=sum;
            
        }
    }
}
