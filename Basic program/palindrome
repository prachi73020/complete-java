import java.util.*;
class Main
{
    public static void main(String []args)
    {
        Scanner sc=new Scanner(System.in);
        int x= sc.nextInt();
        int y=x;
        int n;
        int sum=0;

        while(x>0)
        {
            n=x%10;
            sum=sum*10+n;
            x=x/10;
        }
        
        if(y==sum)
        {
            System.out.println("Palindrome");
        }
        else
        {
            System.out.println("not palindrome");
        }
    }
