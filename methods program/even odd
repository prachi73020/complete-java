//even and odd
import java .util.*;
public class Main
{
    public static void main (String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        evenodd(x);
    } 
    public static void evenodd(int x)
    {
        if(x%2==0)
        System.out.println("even");
        else
       System.out.println("odd");
    }
}
