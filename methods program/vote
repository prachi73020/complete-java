import java.util.*;
public class Main 
{
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        vote(x);
    }
    public static void vote(int x)
    {
        if(x>=18)
        {
            System.out.println("eligible for vote");
        }
        else
        {
            System.out.println("not eligible for voting");
        }
    }
}
