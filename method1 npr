public class Main 
{
    int fact(int x)
    {
        int f=1;
        for(int i=1;i<n;i++)
        {
            f=f*i;
        }
        return f;
    }
    public static void main(String []aegs)
    {
        scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int r=sc.nextInt();
        int nfact=fact(n);
        int rfact=fact(n-r);
        
        int npr=nfact/rfact;
        System.out.println(n+"p"+r+" "+npr);
    }
}
